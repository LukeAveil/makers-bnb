<h1>Spaces</h1>

<ul id='spaces'>
  <% @spaces.each do |space| %>
    <li>
      Name: <%= space.name %>
      <br>
      Price:   <%= space.price   %>
      <br>
      Description:  <%= space.description %>
      <br>
      <a href="spaces/<%=space.id%>">view space</a>
    </li>
  <% end %>
</ul>

<a href="listSpace">List new space</a>
